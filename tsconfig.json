{
    "compilerOptions": {
        "target": "es6",
        "module": "CommonJS",
        // "strict": true,
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "esModuleInterop": true,
        "baseUrl": ".",
        "outDir": "./dist",
        "paths": {
            "*": ["node_modules/*"]
        }
    },

    "include": ["src/**/*", "__tests__/unit/oldTestFile.ts"],
    "exclude": ["node_modules", "dist", "tests"]
}
